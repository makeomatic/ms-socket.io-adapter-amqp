    "@types/common-errors": "^1.0.1",
    "@types/debug": "^4.1.5",
    "@types/is": "^0.0.21",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.6",
