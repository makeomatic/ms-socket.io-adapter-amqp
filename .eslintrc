{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-param-reassign": [2, {"props": false}],
    "no-underscore-dangle": 0,
    "global-require": 0,
    "prefer-arrow-callback": 0,
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "never",
      "exports": "never",
      "functions": "never",
    }]
  }
}
